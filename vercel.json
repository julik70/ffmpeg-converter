{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ],
  "functions": {
    "api/convert.js": {
      "maxDuration": 30
    }
  },
  "env": {
    "FFMPEG_PATH": "/usr/bin/ffmpeg"
  }
}